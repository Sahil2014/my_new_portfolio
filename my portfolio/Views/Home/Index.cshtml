                    <div class="modal-footer">
                        <div class="row">
                            <div class="col-md-6">
                                <button type="button" id="closefb"class="btn btn-success" data-dismiss="modal">Close</button>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12" id="fbcode">
                            <pre class="brush: js">  $("#fbout").on("click", function () {
        fbnum1 = parseInt($("#fbinput1").val());
        fbnum2 = parseInt($("#fbinput2").val());
        let outa = [];
        for (i = 1; i <= 100; i++) {
            if (i % fbnum1 == 0 && i % fbnum2 == 0) {
                outa.push('<span class="fizzbuzz">FizzBuzz </span>');
            } else if (i % fbnum1 == 0) {
                outa.push('<span class="fizz">Fizz</span>');
            } else if (i % fbnum2 == 0) {
                outa.push('<span class="buzz">Buzz</span>');
            } else { outa.push(i); }
        }
        out = outa.join(' ');

        $("#fboutput").html(out);
    });

</pre>

